        },
        {
          "path": "/editor-layout.hbs",
          "text": "<!DOCTYPE html>\n<html>\n<head>\n<meta charset=\"UTF-8\">\n<title>pub editor</title>\n<meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n<link rel=\"stylesheet\" href=\"{{relPath}}/pub/css/normalize.css\">\n<link rel=\"stylesheet\" href=\"{{relPath}}/pub/css/editor.css\">\n<link rel=\"stylesheet\" href=\"{{relPath}}/pub/humane-js/flatty.css\">\n</head>\n<body>\n<div class=\"handle leftright\"></div>\n\n<div class=\"outer col\">\n\n<div class=\"header row\">\n  <!--div class=\"menubutton col clickable\"> ☰ </div-->\n  <div class=\"savebutton col clickable hide\" title=\"save\"> ✓ </div>\n  <div class=\"name col\"></div>\n  <div class=\"editbutton col clickable\" title=\"fragments\"> ✍ </div>\n  <!--div class=\"helpbutton col clickable\" title=\"help\"> ? </div-->\n</div>\n\n<div class=\"main row\">\n  <div class=\"editorpane left col\">\n    <div class=\"list row shadow\"></div>\n    <!--iframe class=\"form row shadow\" src=\"{{relPath}}/admin/upload\"></iframe-->\n    <textarea class=\"editor row\"></textarea></div>\n  <div class=\"previewpane right col\"><iframe class=\"preview\" src=\"{{relPath}}/\"></iframe></div>\n</div>\n\n</div>\n\n<script src=\"{{relPath}}/js/jquery-1.11.3.min.js\"></script>\n<script src=\"{{relPath}}/pub/js/editor-ui.js\"></script>\n<script src=\"{{relPath}}/pub/_generator.js\"></script>\n\n</body>\n</html>\n"
        },
        {
          "path": "/editor-updates.hbs",
          "text": "<ul>\n{{#each diffs}}\n<li title=\"{{difftext}}\" data-file=\"{{file}}\">{{diffpage}}</li>\n{{/each}}\n</ul>\n"
        },
        {
          "path": "/editor-upload.hbs",
          "text": "<!DOCTYPE html>\n<html>\n<head>\n<meta charset=\"UTF-8\">\n<title>pub editor - {{title}}</title>\n<meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n<link rel=\"stylesheet\" href=\"{{relPath}}/css/normalize.css\">\n<link rel=\"stylesheet\" href=\"{{relPath}}/css/editor.css\">\n</head>\n<body>\n<div class=\"upload\">\n<h3>{{title}}</h3>\n<form method=\"POST\" enctype=\"multipart/form-data\">\n{{{html}}}\n</form>\n{{#eachUpload}}<pre>![](/images/{{{title}}}){{s3Err}}\n</pre>\n<img src=\"{{option 'fqImages'}}/images/{{title}}\"><br>\n{{option 'fqImages'}}/images/{{title}}\n{{/eachUpload}}\n</div>\n<script src=\"{{relPath}}/pub/js/jquery-1.11.2.min.js\"></script>\n</body>\n</html>\n"
        }
