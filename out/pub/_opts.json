        {
          "path": "/default.hbs",
          "text": "<header class=\"caption\">\n{{{html}}}\n</header>\n\n{{#each _fragments}}\n<section id=\"{{fragmentID}}\" class=\"slide{{class}}\"><div>\n{{{html}}}\n{{{background-image}}}\n</div></section>\n\n{{/each}}\n\n<div class=\"progress\"><div></div></div>\n"
        },
        {
          "path": "/doc-layout.hbs",
          "text": "<!DOCTYPE html>\n<html {{{lang}}}>\n<head>\n<meta charset=\"utf-8\">\n<meta name=\"viewport\" content=\"width=792\">\n<meta http-equiv=\"x-ua-compatible\" content=\"ie=edge\">\n{{{metaSeo}}}\n\n<!-- html generated by pub-server from markdown {{_file.path}} -->\n\n<title>{{title}}</title>\n{{{injectCss}}}\n</head>\n<body class=\"list\">\n{{{renderLayout}}}\n\n{{{injectJs}}}\n{{{comment \"photoCredit\"}}}\n{{{comment \"copyright\"}}}\n</body>\n</html>\n"
        },
        {
          "path": "/main-layout.hbs",
          "text": "<div {{{layout-class}}} {{{rtl}}}>\n\n<nav id=\"nav\">\n<ul><li>{{menu}}\n<ul>\n{{#eachPage~}}\n<li>{{{pageLink}}}</li>\n{{/eachPage~}}\n</ul>\n</li></ul>\n</nav>\n\n{{{renderPage}}}\n{{{githubBadge}}}\n</div>\n"
        }
      ]
